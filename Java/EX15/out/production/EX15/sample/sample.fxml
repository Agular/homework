<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="108.0" minHeight="70.0" prefHeight="84.0" />
      <RowConstraints maxHeight="509.0" minHeight="471.0" prefHeight="495.0" />
   </rowConstraints>
   <children>
      <Pane fx:id="gameArea" prefHeight="500.0" prefWidth="700.0" GridPane.rowIndex="1" />
      <AnchorPane fx:id="menuArea" prefHeight="79.0" prefWidth="200.0">
         <children>
            <Button fx:id="startButton" layoutX="584.0" layoutY="2.0" mnemonicParsing="false" prefHeight="79.0" prefWidth="116.0" text="START!" />
            <Label fx:id="timerLabel" layoutX="319.0" layoutY="2.0" prefHeight="79.0" prefWidth="81.0" text="Label" />
            <Label fx:id="scoreLabel" layoutX="503.0" layoutY="3.0" prefHeight="79.0" prefWidth="81.0" text="Score" />
            <Label layoutX="460.0" layoutY="28.0" prefHeight="28.0" prefWidth="43.0" text="SCORE">
               <font>
                  <Font name="Calibri Italic" size="14.0" />
               </font>
            </Label>
            <Label layoutX="276.0" layoutY="34.0" prefHeight="17.0" prefWidth="43.0" text="TIME">
               <font>
                  <Font name="Calibri" size="14.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</GridPane>
